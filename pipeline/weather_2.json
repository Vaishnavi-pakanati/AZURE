{
	"name": "weather_2",
	"properties": {
		"activities": [
			{
				"name": "Weather_2",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/mounikavaishu@outlook.com/get_max_date_for_incremental_load",
					"baseParameters": {
						"folder_name": "weather_2"
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "string_",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "temp",
					"value": "'"
				}
			},
			{
				"name": "Weather_2_Data",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Weather_2",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "string_",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MySqlSource",
						"query": {
							"value": "@concat('select * from  weather_2 where date_time >',variables('temp'),activity('Weather_2').output.runOutput,variables('temp'))",
							"type": "Expression"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "MySqlTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "datalake",
						"type": "DatasetReference",
						"parameters": {
							"folder": {
								"value": "@concat('weather/weather_2/as_on_dt=',formatDateTime(utcnow(),'yyyy-MM-dd'))",
								"type": "Expression"
							},
							"file_name": "weather_2"
						}
					}
				]
			},
			{
				"name": "On_Failure",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "Weather_2_Data",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://lgappvaish.azurewebsites.net:443/api/vaishuuu/triggers/When_a_HTTP_request_is_received/invoke?api-version=2022-05-01&sp=%2Ftriggers%2FWhen_a_HTTP_request_is_received%2Frun&sv=1.0&sig=BwoyTxHJpOStIf3IHXGEeqBX1VVRqXK_yBxEagh1AWA",
					"method": "POST",
					"body": {
						"value": "{\n    \"pipeline\":\"@{pipeline().Pipeline}\",\n    \"run_id\":\"@{pipeline().RunId}\",\n    \"time\":\"utcNow()\"\n    \n}",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"temp": {
				"type": "String"
			}
		},
		"annotations": []
	}
